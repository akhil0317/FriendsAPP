<div class="container">
  <h1 class="h1">Friend Details</h1>
  <span style="visibility: hidden">TeachersID is :{{teacherId}}</span>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">id</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">age</th>
        <th scope="col">gender</th>
        <th scope="col">FriendId</th>
      </tr>
    </thead>
    <tbody>

      {{#each data}}
      <tr>

        <th scope="row">{{this.id}}</th>

        <td>{{this.firstName}}</td>
        <td>{{this.lastName}}</td>

        <td>{{this.age}}</td>
        <td>{{this.gender}}</td>
        <td>{{this.friendId}}</td>


        {{!-- <td><a href="/edit-students/{{../teacherId}}/{{this.id}}">Edit</a></td> --}}
      </tr>
      {{/each}}


    </tbody>
  </table>

  {{!-- <a href="/add-students/{{teacherId}}" class=" btn btn-primary btn-lg">Add Student</a> --}}
</div>